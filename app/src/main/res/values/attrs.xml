<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Polygon">
        <attr name="sides" format="integer" />
        <attr name="stroke_color" format="color" />
        <attr name="stroke_width" format="integer" />
        <attr name="fill_color" format="color" />
        <attr name="start_angle" format="float" />
        <attr name="inscribed_circle" format="boolean" />
        <attr name="fill_percent" format="float" />
        <attr name="fill_bitmap" format="integer" />
    </declare-styleable>



        <declare-styleable name="SlidingUpPanelLayout">
            <attr name="panelHeight" format="dimension" />
            <attr name="shadowHeight" format="dimension" />
            <attr name="paralaxOffset" format="dimension" />
            <attr name="fadeColor" format="color" />
            <attr name="flingVelocity" format="integer" />
            <attr name="dragView" format="reference" />
            <attr name="overlay" format="boolean"/>
        </declare-styleable>


</resources>